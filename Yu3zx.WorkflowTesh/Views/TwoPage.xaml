<Page
    x:Class="Yu3zx.WorkflowTesh.Views.TwoPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Yu3zx.WorkflowTesh.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="TwoPage"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">

    <Grid>
        <TextBlock
            Margin="15"
            HorizontalAlignment="Center"
            FontSize="36"
            Text="第二页" />
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">

            <CheckBox
                x:Name="chkResult"
                Margin="25"
                Content="是否直接结束"
                FontSize="25" />

            <Button
                Width="128"
                Height="36"
                Margin="25"
                Click="Button_Click"
                Content="提 交"
                FontSize="24" />
        </StackPanel>

    </Grid>
</Page>
